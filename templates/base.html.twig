        <nav class="navbar navbar-bg align-items-end">
            <a class="navbar-brand" href="{{ path('app_homepage') }}"><img class="d-inline-block align-top navbar-brand" src="../images/brand.svg" alt="Brand"></a>
            <button type="button" class="btn btn-outline-default button-size">
                <i class="fa fa-plus"></i> Zaloguj Się
            </button>
        </nav>

        <nav class="navbar navbar-expand-lg navbar-light bg-light sticky-top">
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarNav">
                <ul class="navbar-nav mr-auto">
                    <li class="nav-item">
                        <a class="nav-link dropdown-toggle" href="{{ path('app_products', {'typesid': 'all'}) }}" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            Produkty
                        </a>
                        <div class="dropdown-menu text-center" aria-labelledby="navbarDropdown">
                            <h5 class="row">
                                <div class="col-sm-4">
                                    <a class="dropdown-item" href="{{ path('app_products', {'typesid': 'all'}) }}"><i class="fa fa-cube" style="color: cadetblue"></i> Wszystkie</a>
                                </div>
                                <div class="col-sm-4">
                                    <a class="dropdown-item" href="#"><i class="fa fa-star" style="color: orange"></i> Nowości</a>
                                </div>
                                <div class="col-sm-4">
                                    <a class="dropdown-item" href="#"><i class="fa fa-percent" style="color: darkred"></i> Wyprzedaż</a>
                                </div>
                            </h5>
                            <div class="dropdown-divider"></div>
                            <div class="row">
                                {% for type in types %}
                                    <div class="col-sm-3">
                                        <a class="dropdown-item" href="{{ path('app_products', {'typesid': type.id}) }}">{{ type.name }}</a>
                                    </div>
                                {% endfor %}
                            </div>
                        </div>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="{{ path('app_contact') }}">Kontakt</a>
                    </li>
                </ul>
                <form class="form-inline my-2 my-lg-0">
                    <input class="form-control mr-sm-2" type="search" placeholder="Szukaj" aria-label="Search">
                    <button class="btn btn-secondary my-2 my-sm-0 button-size" type="submit">
                        <i class="fa fa-search"></i> Szukaj
                    </button>
                </form>

                <div>
                    <button type="button" class="btn btn-secondary dropdown-toggle ml-2 button-size" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                        <i class="fa fa-shopping-cart"></i> Koszyk
                    </button>
                    <div class="dropdown-menu">
                        <a class="dropdown-item" href="#">Action</a>
                        <a class="dropdown-item" href="#">Another action</a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="#">Something else here</a>
                    </div>
                </div>
            </div>
        </nav>

